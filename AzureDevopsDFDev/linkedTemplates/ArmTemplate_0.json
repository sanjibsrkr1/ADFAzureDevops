						"typeProperties": {
							"packageLocation": {
								"packagePath": "//ssisdb/test/test/package1.dtsx",
								"type": "SSISDB"
							},
							"connectVia": {
								"referenceName": "adfIntegrationRunTime",
								"type": "IntegrationRuntimeReference"
							},
							"loggingLevel": "Basic",
							"projectConnectionManagers": {},
							"packageConnectionManagers": {}
						}
					},
					{
						"name": "IR-Stop",
						"type": "WebActivity",
						"dependsOn": [
							{
								"activity": "Execute SSIS package",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
