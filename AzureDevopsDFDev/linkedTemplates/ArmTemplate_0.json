							"method": "POST",
							"headers": {},
							"body": {
								"name": "test",
								"salary": "123",
								"age": "23"
							},
							"authentication": {
								"type": "MSI",
								"resource": "https://management.azure.com/"
							}
						}
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/adfIntegrationRunTime')]"
